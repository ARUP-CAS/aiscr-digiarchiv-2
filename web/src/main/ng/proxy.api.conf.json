{
  "/api/**": {
    "target": "http://digiarchiv:8080/api",
    "logLevel": "debug",
    "pathRewrite": {
      "^/api":""
    },
    "changeOrigin": false,
    "withCredentials": true,
    "secure": false
  },
  
  "/assets/i18n/**": {
    "target": "http://digiarchiv:8080/assets/i18n/cs",
    "logLevel": "debug",
    "pathRewrite": {
      "^/assets/i18n":""
    },
    "changeOrigin": false,
    "secure": false
  },
  
  "/assets/config_.json": {
    "target": "http://digiarchiv:8080/assets/config.json",
    "pathRewrite": {
      "^/api":""
    },
    "changeOrigin": false,
    "secure": false
  }
}
