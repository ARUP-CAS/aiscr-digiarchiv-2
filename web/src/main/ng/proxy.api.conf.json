{
  "/api": {
    "target": "http://localhost:8080/api",
    "logLevel": "debug",
    "changeOrigin": true,
    "withCredentials": true,
    "secure": false
  },
  
  "/assets/i18n/**": {
    "target": "http://localhost:8080/assets/i18n",
    "logLevel": "debug",
    "pathRewrite": {
      "^/assets/i18n":""
    },
    "changeOrigin": false,
    "secure": false
  },
  
  "/assets/config.json": {
    "target": "http://localhost:8080/assets/config.json",
    "pathRewrite": {
      "^/api":""
    },
    "changeOrigin": false,
    "secure": false
  }
}
